<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    tools:context=".Command_enter">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="229dp"
        android:layout_height="474dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText3"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.744"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pngwing" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="216dp"
        android:layout_height="431dp"
        android:layout_marginTop="-120dp"
        android:layout_marginEnd="80dp"
        android:rotation="90"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pngwing" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="216dp"
        android:layout_height="431dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="80dp"
        android:rotation="90"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pngwing" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="216dp"
        android:layout_height="431dp"
        android:layout_marginEnd="96dp"
        android:layout_marginBottom="-180dp"
        android:rotation="90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editTextText4"
        app:srcCompat="@drawable/pngwing" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="216dp"
        android:layout_height="431dp"
        android:layout_marginEnd="96dp"
        android:layout_marginBottom="-5dp"
        android:rotation="90"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="@+id/editTextText4"
        app:srcCompat="@drawable/pngwing" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="229dp"
        android:layout_height="474dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pngwing" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="48dp"
        android:text="@string/command_1"
        android:textAllCaps="false"
        android:textColor="@color/red"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="16dp"
        android:text="@string/command_2"
        android:textColor="@color/blue"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="26dp"
        android:text="@string/command_3"
        android:textColor="@color/green"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="33dp"
        android:text="@string/command_4"
        android:textColor="@color/yellow"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:insetBottom="0dp"
        android:text="@string/next_emoji"
        android:textColor="@color/bg"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="180sp"
        android:layout_height="50sp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="100dp"
        android:autofillHints="username"
        android:ems="10"
        android:hint="@string/command_name"
        android:inputType="text"
        android:textColor="@color/red"
        android:textColorHint="@color/text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="180sp"
        android:layout_height="50sp"
        android:layout_marginTop="100dp"
        android:autofillHints="username"
        android:ems="10"
        android:hint="@string/command_name"
        android:inputType="text"
        android:textColor="@color/blue"
        android:textColorHint="@color/text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="180sp"
        android:layout_height="50sp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="100dp"
        android:autofillHints="username"
        android:ems="10"
        android:hint="@string/command_name"
        android:inputType="text"
        android:textColor="@color/green"
        android:textColorHint="@color/text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="180sp"
        android:layout_height="50sp"
        android:layout_marginTop="100dp"
        android:autofillHints="username"
        android:ems="10"
        android:hint="@string/command_name"
        android:inputType="text"
        android:textColor="@color/yellow"
        android:textColorHint="@color/text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

</androidx.constraintlayout.widget.ConstraintLayout>
